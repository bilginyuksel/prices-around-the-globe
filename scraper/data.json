  {
    "id": "truecar-text",
    "iso": "us",
    "baseURL": "https://www.truecar.com/new-cars-for-sale/listings/",
    "selector": ".row li",
    "type": "TEXT",
    "queryText": [
      {
        "children": [
          "div",
          ".vehicle-card",
          ".card-content",
          ".vehicle-card-top",
          "h3",
          ".vehicle-card-header",
          ".vehicle-header-make-model"
        ],
        "key": "brand-model"
      },
      {
        "children": [
          "div",
          ".vehicle-card",
          ".card-content",
          "div.card-content.vehicle-card-body.order-3 > div:nth-child(2)",
          ".vehicle-card-bottom",
          ".vehicle-card-bottom-pricing",
          ".vehicle-card-bottom-pricing-primary",
          "div",
          ".heading-4",
          "span"
        ],
        "key": "price"
      },
      {
        "children": [
          "div",
          ".vehicle-card",
          ".card-content",
          ".vehicle-card-top",
          "h3",
          ".font-size-1"
        ],
        "key": "characteristics"
      }
    ]
  },
  {
    "id": "araba-query",
    "baseURL": "https://araba.com/arama?q={q}",
    "selector": "body > div.page-container > div.page-wrapper > div.page-content > div > div > div:nth-child(1) > div.gsr-category-result > ul > li:nth-child(1) > a",
    "type": "ATTR",
    "next": "araba-text",
    "attr": "href"
  },
  {
    "id": "araba-text",
    "iso": "tr",
    "baseURL": "https://www.araba.com",
    "type": "TEXT",
    "next": null,
    "selector": "table tbody tr",
    "queryText":[
      {
        "children":[
          "td.brand",
          "a"
        ],
        "key":"brand-model-characteristics"
      },
      {
        "children":[
          "td.year",
          "a"
        ],
        "key":"year"
      },
      {
        "children":[
          "td.milage",
          "a"
        ],
        "key":"milage"
      },
      {
        "children":[
          "td:nth-child(8)",
          "div.prices",
          "div"
        ],
        "key":"price"
      }
    ]
  }
]